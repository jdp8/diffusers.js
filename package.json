{
  "name": "@aislamov/diffusers.js",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@aislamov/onnxruntime-web64": "^1.0.2",
    "@huggingface/hub": "^0.8.5",
    "@types/node": "^16.18.34",
    "@xenova/transformers": "^2.6.2",
    "idb": "^7.1.1",
    "onnxruntime-common": "1.16.1",
    "onnxruntime-node": "1.16.1",
    "seedrandom": "^3.0.5",
    "sharp": "^0.32.6",
    "wasm-feature-detect": "^1.5.1"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:types",
    "build:ts": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "eject": "react-scripts eject",
    "setup": "node examples/react/ort-wasm-handler.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.esm.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "url": false,
    "sharp": false,
    "util": false,
    "onnxruntime-node": false,
    "stream/web": false
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.195",
    "@types/seedrandom": "^3.0.6",
    "esbuild": "0.19.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "@xenova/transformers": {
      "onnxruntime-common": "1.16.1",
      "onnxruntime-node": "1.16.1"
    }
  }
}
